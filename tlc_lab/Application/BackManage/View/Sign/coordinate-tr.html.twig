<tr id="coordinate-tr-{{ coordinate.id }}" class="coordinate-tr">
    <td>{{ coordinate.id }}</td>
    <td>{{ coordinate.name }}</td>
    <td>{{ coordinate.longitude }}</td>
    <td>{{ coordinate.latitude }}</td>
    <td>
        {% if coordinate.createTime > 0 %}{{ coordinate.createTime|date("Y-n-d H:i") }}{% else %} <span class="text-muted">——</span>{% endif %}
    </td>
    <td>
        {% if coordinate.updateTime > 0 %}{{ coordinate.updateTime|date("Y-n-d H:i") }}{% else %} <span class="text-muted">——</span>{% endif %}
    </td>
    <td>
        <div class="btn-group">
            <a class="btn btn-default btn-sm" href="#modal" data-toggle="modal" data-url="{{ path('admin_sign_coordinate_edit', {id:coordinate.id}) }}">编辑</a>
            <a href="#" type="button" class="btn btn-default btn-sm dropdown-toggle" data-toggle="dropdown">
                <span class="caret"></span>
            </a>
            <ul class="dropdown-menu pull-right">
                <li><a class="remove-category" href="#modal" data-toggle="modal" data-url="{{ path('admin_sign_coordinate_remove', {id:coordinate.id}) }}"><span class="glyphicon glyphicon-trash"></span> 删除分类</a></li>
            </ul>
        </div>
    </td>
</tr>