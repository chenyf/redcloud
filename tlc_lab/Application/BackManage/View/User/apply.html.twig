{% extends '@BackManage/User/layout.html.twig' %}

{% block title %}学号申请管理 - {{ parent() }}{% endblock %}

{% set menu = 'user-apply' %}


{% block main %}
<div class="page-header">
  <h1>学号申请管理</h1>
</div>
{% include '@BackManage/User/apply-tab.html.twig' %}
<table id="teacher-table" class="table table-striped table-hover" data-search-form="#user-search-form">
  <thead>
    <tr>
      <th>ID</th>
      <th>申请时间</th>
      <th>申请者</th>
      <th>邮箱/手机</th>
      <th>状态</th>
      <th>审核者</th>
      <th>审核时间</th> 
      <th>操作</th>
    </tr>
  </thead>
  <tbody>
    {% if applys %}
      {% for apply in applys %}
        {% include '@BackManage/User/apply-tr.html.twig' with {apply:apply} %}
      {% endfor %}
    {% else %}
       <tr><td colspan="20"><div class="empty">暂无学号申请记录</div></td></tr>
    {% endif %}
  </tbody>
</table>
 {{ web_macro.paginator(paginator) }} 
{% endblock %}