{% extends '@BackManage/System/layout.html.twig' %}

{% block title %}app配置信息 - {{ parent() }}{% endblock %}

{% set script_controller = 'mobile/module-list' %}

{% set menu = 'mobile_infocfg' %}

{% block main %}

<div class="page-header clearfix"> 
   <button class="btn btn-info btn-sm pull-right" id="add-navigation-btn" data-backdrop="static" data-toggle="modal" data-target="#modal" data-url="{{ path('admin_mobile_info_cfg_edit') }}"><span class="glyphicon glyphicon-plus"></span> 添加配置
    </button>
  <h1 class="pull-left">app配置信息</h1>
</div>


<div id="aticle-table-container">
<table class="table table-hover table-striped" id="article-table">
    <thead>
        <tr>
            <th > ID</th>
            <th >配置键名</th>
            <th >含义</th>
            <th width="10%">是否支持后台设置</th>
            <th >配置值</th>
            <th >最后修改人</th>
            <th >操作</th>
        </tr>
    </thead>
    <tbody>
        {% if infoCfgList %}
            {% for version in infoCfgList %}
              {% include '@BackManage/Mobile/infoCfg-tr.html.twig' %}
            {% endfor %}
        {% else %}
            <tr><td colspan="20"><div class="empty">暂无页面记录</div></td></tr>
        {% endif %}
    </tbody>
    </table>
  </div>
    {{ web_macro.paginator(paginator) }}
{% endblock %}