{% extends '@BackManage/System/layout.html.twig' %}

{% block title %}app版本管理 - {{ parent() }}{% endblock %}

{% set script_controller = 'mobile/list' %}

{% set menu = 'app_version' %}

{% block main %}

<div class="page-header clearfix">
   
<h1 class="pull-left">app版本管理</h1>
</div>


<div id="aticle-table-container">
<table class="table table-hover table-striped" id="article-table">
	<thead>
		<tr>
			<th > ID</th>
			<th >名称</th>
			<th >版本</th>
			<th >类型</th>
			<th >链接</th>
			<th >强制升级</th>
			<th >是否生效</th>
			<th >创建时间</th>
			<th >修改时间</th>
			<th >操作</th>
		</tr>
	</thead>
	<tbody>
		{% if versions %}
			{% for version in versions %}
			  {% include '@BackManage/Mobile/version-tr.html.twig' %}
			{% endfor %}
		{% else %}
			<tr><td colspan="20"><div class="empty">暂无页面记录</div></td></tr>
		{% endif %}
	</tbody>
	</table>
  </div>
    {{ web_macro.paginator(paginator) }}
{% endblock %}