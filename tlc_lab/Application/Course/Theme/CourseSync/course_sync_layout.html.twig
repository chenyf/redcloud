{% extends '@My/My/layout.html.twig' %}

{% block title %}同步课程 - {{ parent() }}{% endblock %}

{% set side_nav = 'upload-course' %}

{% block main %}
    <div class="panel panel-default panel-col">

        <div class="panel-body cc-panel-body">

            <ul class="nav nav-tabline">
                <li{% if tab_nav == 'upload' %} class="active"{% endif %}><a href="{{ path('course_sync') }}">上传新课程</a></li>
                <li{% if tab_nav == 'record' %} class="active"{% endif %}><a href="{{ path('course_sync_record') }}">上传记录</a></li>
            </ul>

            {% block main_content %}{% endblock %}

        </div>
    </div>
{% endblock %}