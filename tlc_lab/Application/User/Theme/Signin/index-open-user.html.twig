{% extends '@User/layout.html.twig' %}
{% block title %}登录 - {{ parent() }}{% endblock %}
{% set script_controller = 'auth/login' %}
{% set containerClassBlock = 'index_container' %}
{% set two_nav = '欢迎登录' %}
{% block content %}
<div class="login-bg" style="background-image:url({{login_pic_path()}});">
    <div class="redcloud-bggray">
        <div class="container">
            <div class="row row-6 mtl">
                <div class="col-md-4 col-md-offset-4">  
                    <div class="panel panel-default panel-page" {% if isLogin == false %} style="padding:10px 0 10px"{% endif %}>
                        {% include '@User/Signin/login-open-user-form.html.twig' with {'studNumLogin':studNumLogin,'errorNum':errorNum}%}
                        <div style="overflow:hidden;clear:both;margin:0 50px;">
                        </div>
                        <div class="social-logins"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
{% endblock %}