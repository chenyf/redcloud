{% extends '@BackManage/Course/layout.html.twig' %}
{% set menu = 'GeneralizeLevel' %}
 
{% block main %}

        <div class="page-header">
          <h1>提成等级</h1>
        </div> 
  <table class="table table-striped table-hover" id="order-table">
    <thead>
      <tr>
        <th>ID</th>
        <th>等级</th>
        <th>简介</th>
        <th>更新时间</th>
      </tr>
    </thead>
    <tbody>
      {% for generalizeLevels in generalizeLevel %}
       <tr>
            <td>{{generalizeLevels.id}}</td>
            <td>{{generalizeLevels.title}}</td>
            <td>{{ generalizeLevels.info}}</td>
            <td>{{ generalizeLevels.ctime | date('Y/m/d H:i:s')}}</td>
       </tr>
      {% endfor %}
    </tbody>
    
  </table>

  {{ web_macro.paginator(paginator) }} 
 
{% endblock %}
