{% extends '@BackManage/OperationAnalysis/analysis-base-layout.html.twig' %}
{% set script_controller = 'analysis/task-notice' %}
{% set href="admin_operation_analysis_task_notice" %}
{% set menu = 'taskNotice' %}
{% block analysisBody %}
  <div class="col-md-12">  
              {% if tab=="trend" %}
	 <div id="line-data"></div>
	           <input id="data"  type="hidden" value='{% if data %}{{data}}{% endif %}'>  
	          </input>
	          <input id="taskStartData"  type="hidden" value='{% if taskStartData %}{{taskStartData}}{% endif %}'>  
	          </input>
              {% elseif tab=="detail" %}
                    {% include '@BackManage/OperationAnalysis/task-notice.table.html.twig' %}
              {% endif %}
  </div>

{% endblock %}
   